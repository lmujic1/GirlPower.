@model IEnumerable<GirlPower.Models.Tretman>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.OpisTretmana)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.CijenaTretmana)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.TrajanjeTretmana)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Izvrsen)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.NazivTretmana)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.OpisTretmana)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CijenaTretmana)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.TrajanjeTretmana)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Izvrsen)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.NazivTretmana)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.Id">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.Id">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
